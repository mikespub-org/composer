{
    "name": "mikespub/composer",
    "homepage": "https://github-org.mikespub.net/composer/",
    "description": "Mike's Pub Composer Packages",
    "output-dir": "docs",
    "include-filename": "all.json",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/php-epub-meta"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/epub-loader"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/intity-epubjs-reader"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/seblucas-cops"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/kiwilan-php-opds"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/rvolz-BicBucStriim"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/middlewares-cache"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/middlewares-utils"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/micheh-psr7-cache"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/survos-wikidata"
        }
    ],
    "require": {
        "kiwilan/php-opds": "^2.2.0",
        "mikespub/epub-loader": "^3.6.4",
        "mikespub/epubjs-reader": "^2025.07.07",
        "mikespub/php-epub-meta": "^3.5.1",
        "mikespub/seblucas-cops": "^3.7.1",
        "mikespub/micheh-psr7-cache": "^2.0.3",
        "mikespub/middlewares-cache": "^3.1.0",
        "middlewares/utils": "^4.0.2",
        "mikespub/rvolz-bicbucstriim": "^3.6.5",
        "mikespub/survos-wikidata": "^5.2.5"
    },
    "require-dependencies": false,
    "require-dev-dependencies": false,
    "archive": {
        "directory": "dist",
        "skip-dev": false
    }
}
