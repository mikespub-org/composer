{
    "name": "mikespub/composer",
    "homepage": "https://github-org.mikespub.net/composer/",
    "description": "Mike's Pub Composer Packages",
    "output-dir": "docs",
    "include-filename": "all.json",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/php-epub-meta"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/epub-loader"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/seblucas-cops"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/kiwilan-php-opds"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/rvolz-BicBucStriim"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/middlewares-cache"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mikespub-org/micheh-psr7-cache"
        }
    ],
    "require": {
        "kiwilan/php-opds": "dev-paging",
        "mikespub/epub-loader": "^2.2.0",
        "mikespub/php-epub-meta": "^2.2.0",
        "mikespub/seblucas-cops": "^2.2.1",
        "mikespub/micheh-psr7-cache": "^2.0.0",
        "mikespub/middlewares-cache": "^2.2.0",
        "mikespub/rvolz-bicbucstriim": "^3.1.0"
    },
    "require-dependencies": false, 
    "require-dev-dependencies": false, 
    "archive": {
        "directory": "dist",
        "skip-dev": false
    }
}
